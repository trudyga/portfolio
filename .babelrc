{
  "presets": [
    "@babel/env",
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
    ["react-intl", {
      "messagesDir": "./public/messages/"
    }]
  ]
}